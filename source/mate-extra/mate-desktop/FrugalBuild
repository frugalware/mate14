pkgname=mate-desktop
pkgver=1.5.6
pkgrel=2
pkgdesc="Library with common API for various MATE modules"
archs=('i686' 'x86_64')
groups=('mate-extra' 'mate-base')
depends=('unique1' 'libmatenotify' 'hicolor-icon-theme' 'gvfs-obexftp' 'startup-notification')
makedepends=('mate-doc-utils' 'intltool')
_F_mate_iconcache="y"
_F_mate_glib="y"
_F_mate_desktop="y"
Finclude mate mate-scriptlet
sha1sums=('3a7df1dffde2241d265967ec197585c877ea0dac')

build()
{
	Fcd
	./autogen.sh || Fdie
	Fmake
	Fmakeinstall
	Fbuild_mate_scriptlet
}

# optimization OK
