pkgname=mate-panel
pkgver=1.5.4
pkgrel=1
pkgdesc="The MATE Desktop panel applets"
archs=('i686' 'x86_64')
groups=('mate-extra' 'mate-base')
depends=('mate-session-manager' 'gsettings-desktop-schemas')
makedepends=('mate-common' 'dconf' 'desktop-file-utils' 'gobject-introspection' \
	'icon-naming-utils' 'libcanberra-gtk2' 'libmateweather' 'mate-desktop' \
	'mate-menus' 'intltool' 'mate-doc-utils' 'pkgconfig' 'libmatewnck' \
	'startup-notification' 'libxres' 'librsvg')
_F_mate_desktop="y"
_F_mate_glib="y"
__F_mate_iconcache="y"
Finclude mate mate-scriptlet
sha1sums=('e7195dab2f479610af39f459a089299ffd1468fd')

build()
{
	Fcd
	Fsed 'AM_CONFIG_HEADER' 'AC_CONFIG_HEADERS' configure.ac
	./autogen.sh || Fdie
	Fmake
	Fmakeinstall
	Fbuild_mate_scriptlet
}

# optimization OK
