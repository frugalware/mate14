groups=('gnome-extra')
archs=('i686' 'x86_64')
depends=('gtk+2' 'perl-gtk2-ex-simple-list' 'perl-gtk2-gladexml' 'perl-yaml'\
	'perl-flickr-api' 'perl-xml-parser-lite-tree' 'perl-libwww'\
	'perl-gettext')
source=("https://github.com/downloads/reidrac/$pkgname/$pkgname-$pkgver.tar.gz")
up2date=("Flasttar $url")
_F_desktop_filename="$pkgname.desktop"
Finclude gnome-scriptlet
